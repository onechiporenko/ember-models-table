    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "start": "ember serve",
    "test": "npm-run-all lint test:*",
    "test:ember": "ember test",
    "test:ember-compatibility": "ember try:each",
    "test:all": "ember try:each",
    "test:cov": "COVERAGE=true ember test",
    "test:cov:default": "COVERAGE=true ember test",
    "test:cov:bs3": "COVERAGE=true ember try:one ember-default-with-ember-bootstrap-v3 --- ember test",
    "test:cov:bs4": "COVERAGE=true ember try:one ember-default-with-ember-bootstrap-v4 --- ember test",
