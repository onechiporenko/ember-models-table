{
  "name": "ember-models-table",
  "version": "3.4.0",
  "description": "Table with pagination, sorting and filtering",
  "keywords": [
    "data-table",
    "ember-addon",
    "table"
  ],
  "repository": "https://github.com/onechiporenko/ember-models-table",
  "license": "MIT",
  "author": "Oleg Nechiporenko",
  "directories": {
    "doc": "docs",
    "test": "tests"
  },
  "scripts": {
    "build:gh-pages": "ember build --prod",
    "build:gh-pages:bs3": "ember try:one ember-default-with-ember-bootstrap-v3 --- ember build --prod",
    "build:gh-pages:bs4": "ember try:one ember-default-with-ember-bootstrap-v4 --- ember build --prod",
    "build:gh-pages:paper": "ember try:one ember-default-with-ember-paper --- ember build --prod",
    "build:gh-pages:plain-html": "ember try:one ember-default-with-plain-html --- ember build --prod",
    "build:gh-pages:docs": "ember build --docs",
    "build": "ember build --environment=production",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*",
    "lint:hbs": "ember-template-lint .",
    "lint:js": "eslint .",
    "start": "ember serve",
    "test": "npm-run-all lint:* test:*",
    "test:ember": "ember test",
    "test:ember-compatibility": "ember try:each",
    "test:all": "ember try:each",
    "test:cov": "COVERAGE=true ember test",
    "test:cov:default": "COVERAGE=true ember test",
    "test:cov:bs3": "COVERAGE=true ember try:one ember-default-with-ember-bootstrap-v3 --- ember test",
    "test:cov:bs4": "COVERAGE=true ember try:one ember-default-with-ember-bootstrap-v4 --- ember test",
    "test:cov:paper": "COVERAGE=true ember try:one ember-default-with-ember-paper --- ember test",
    "test:cov:plain-html": "COVERAGE=true ember try:one ember-default-with-plain-html --- ember test",
    "docs": "ember ember-cli-yuidoc"
  },
  "dependencies": {
    "ember-angle-bracket-invocation-polyfill": "^2.0.2",
    "ember-cli-babel": "^7.22.1",
    "ember-cli-htmlbars": "^5.3.1",
    "ember-cli-string-helpers": "^5.0.0",
    "ember-composable-helpers": "~4.2.2",
    "ember-decorators": "^6.1.1",
    "ember-decorators-polyfill": "^1.1.0",
    "ember-fn-helper-polyfill": "^1.0.2",
    "ember-truth-helpers": "^2.1.0"
  },
  "devDependencies": {
    "@ember/optional-features": "^2.0.0",
    "@ember/test-helpers": "^1.5.0",
    "@glimmer/component": "^1.0.1",
    "@glimmer/tracking": "^1.0.1",
    "babel-eslint": "^10.1.0",
    "broccoli-asset-rev": "^3.0.0",
    "coveralls": "^3.0.9",
    "ember-a11y-testing": "^2.0.0",
    "ember-auto-import": "^1.6.0",
    "ember-cli": "~3.21.0",
    "ember-cli-code-coverage": "1.0.0-beta.8",
    "ember-cli-dependency-checker": "^3.2.0",
    "ember-cli-deprecation-workflow": "^1.0.1",
    "ember-cli-inject-live-reload": "^2.0.2",
    "ember-cli-mirage": "^1.1.8",
    "ember-cli-page-object": "^1.17.3",
    "ember-cli-sass": "^7.2.0",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-uglify": "^3.0.0",
    "ember-cli-yuidoc": "^0.9.1",
    "ember-code-snippet": "^3.0.0",
    "ember-data": "^3.15.0",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-exam": "^5.0.0",
    "ember-export-application-global": "^2.0.1",
    "ember-fetch": "^8.0.2",
    "ember-in-element-polyfill": "^0.3.0",
    "ember-infinity": "^2.1.0",
    "ember-load-initializers": "^2.1.1",
    "ember-maybe-import-regenerator": "^0.1.6",
    "ember-power-select": "^2.3.5",
    "ember-prism": "^0.5.0",
    "ember-qunit": "^4.6.0",
    "ember-qunit-assert-helpers": "^0.2.0",
    "ember-resolver": "^8.0.2",
    "ember-source": "~3.21.1",
    "ember-source-channel-url": "^3.0.0",
    "ember-template-lint": "^2.11.0",
    "ember-try": "^1.4.0",
    "eslint": "^7.8.0",
    "eslint-plugin-ember": "^8.13.0",
    "eslint-plugin-node": "^11.1.0",
    "faker": "^5.1.0",
    "lcov-result-merger": "^3.1.0",
    "loader.js": "^4.7.0",
    "npm-run-all": "^4.1.5",
    "qunit-dom": "^1.4.0"
  },
  "engines": {
    "node": "10.* || >= 12"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "demoURL": "http://onechiporenko.github.io/ember-models-table/",
    "configPath": "tests/dummy/config"
  }
}
