<a
  href="#"
  role="button"
  onclick={{this.doToggleAllSelection}}
  class={{concat this.themeInstance.buttonLink " toggle-all"}}>
  <i class={{if
    (is-equal this.selectedItems.length this.data.length)
    this.themeInstance.selectAllRowsIcon
    this.themeInstance.deselectAllRowsIcon
  }}></i>
</a>
{{yield}}