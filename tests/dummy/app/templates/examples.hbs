<h3>Examples</h3>
{{#if fw.isBs3}}
  <ExamplesNavBs3 @routes={{routes}} />
  {{outlet}}
{{/if}}
{{#if fw.isBs4}}
  <ExamplesNavBs4 @routes={{routes}} />
  {{outlet}}
{{/if}}
{{#if fw.isSemanticUI}}
  <div class="ui grid">
    <div class="three wide column">
      <ExamplesNavSemantic @routes={{routes}} />
    </div>
    <div class="thirteen wide column">
      {{outlet}}
    </div>
  </div>
{{/if}}
{{#if fw.isPaper}}
  <ExamplesNavPaper @routes={{routes}} />
  {{#paper-card as |card|}}
    {{#card.content}}
      {{outlet}}
    {{/card.content}}
  {{/paper-card}}
{{/if}}