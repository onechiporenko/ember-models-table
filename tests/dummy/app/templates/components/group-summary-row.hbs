{{#each @visibleProcessedColumns as |column index|}}
  <td>
    Column #{{index}}.
    <span class="expanded">{{this.expandedGroupedItems.length}}</span>
    <span class="visible">{{this.visibleGroupedItems.length}}</span>
    <span class="selected">{{this.selectedGroupedItems.length}}</span>
  </td>
{{/each}}